generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int    @id @default(autoincrement()) @db.UnsignedInt
  name     String @unique
  password String
}

model article {
  id      Int    @id @default(autoincrement()) @db.UnsignedInt
  title   String
  content String @db.Text
}
